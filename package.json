{
  "name": "farming-hell",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run -s build:data-quick && npm run -s build:typecheck && npm run -s build:esbuild",
    "build:typecheck": "tsc --noEmit -p tsconfig.json",
    "build:esbuild": "node ./esbuild.mjs --production",
    "build:data-full": "ts-node -T --esm src/bin/extract-data.ts",
    "build:data-quick": "ts-node -T --esm src/bin/extract-data.ts --skip-version-check --skip-image-download",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "watch": "node ./esbuild.mjs --watch",
    "serve": "python -m http.server 8080 -b 127.0.0.1 -d _build/dist",
    "test": "jest",
    "test:fix": "jest --update-snapshot",
    "test:watch": "jest --watch"
  },
  "author": "SquidDev",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/sortablejs": "^1.10.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.3.1",
    "esbuild": "^0.19.1",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-glob": "^3.2.11",
    "jest": "^29.0.0",
    "postcss": "^8.3.6",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.0",
    "@headlessui/react": "^1.6.3",
    "@welldone-software/why-did-you-render": "^7.0.0",
    "formik": "^2.2.9",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^4.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-fast-compare": "^3.2.0",
    "sortablejs": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "uuid": "^9.0.0",
    "yup": "^1.0.0-beta.8"
  }
}
