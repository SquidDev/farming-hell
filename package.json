{
  "name": "farming-hell",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run -s build:data-quick && npm run -s build:typecheck && npm run -s build:esbuild",
    "build:typecheck": "tsc --noEmit -p tsconfig.json",
    "build:esbuild": "node ./esbuild.mjs --production",
    "build:data-full": "tsc --outDir _build/ts --module commonjs --target es6 src/bin/extract-data.ts && node _build/ts/bin/extract-data.js",
    "build:data-quick": "tsc --outDir _build/ts --module commonjs --target es6 src/bin/extract-data.ts && node _build/ts/bin/extract-data.js --skip-version-check --skip-image-download",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "watch": "node ./esbuild.mjs --watch",
    "serve": "python -m http.server 8080 -d _build/dist",
    "test": "jest",
    "test:fix": "jest --update-snapshot",
    "test:watch": "jest --watch",
    "postinstall": "patch-package"
  },
  "author": "SquidDev",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/sortablejs": "^1.10.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.3.1",
    "esbuild": "^0.14.23",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-glob": "^3.2.11",
    "jest": "^28.0.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.6",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "@welldone-software/why-did-you-render": "^7.0.0",
    "formik": "^2.2.9",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "rc-tooltip": "^5.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sortablejs": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "uuid": "^8.3.2",
    "vm2": "^3.9.8",
    "yup": "^1.0.0-beta.4"
  }
}
