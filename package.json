{
  "name": "farming-hell",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run -s build:data-quick && npm run -s build:rollup",
    "build:rollup": "rollup -c --environment=NODE_ENV:production",
    "build:data-full": "tsc --outDir _build/ts --module commonjs --target es6 src/bin/extract-data.ts && node _build/ts/bin/extract-data.js",
    "build:data-quick": "tsc --outDir _build/ts --module commonjs --target es6 src/bin/extract-data.ts && node _build/ts/bin/extract-data.js --skip-version-check --skip-image-download",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "watch": "rollup -c --watch --no-watch.clearScreen",
    "serve": "python -m http.server 8080 -d _build/dist",
    "test": "jest",
    "test:fix": "jest --update-snapshot",
    "test:watch": "jest --watch"
  },
  "author": "SquidDev",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@rollup/plugin-url": "^6.1.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/sortablejs": "^1.10.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.3.1",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "postcss": "^8.3.6",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@welldone-software/why-did-you-render": "^6.2.0",
    "formik": "^2.2.9",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^5.0.0-beta.0",
    "sortablejs": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  }
}
